{
  "extends": ["react-app", "eslint:recommended", "plugin:prettier/recommended", "prettier"],
  "rules": {
    "no-var": "error", // var 금지
    "no-multiple-empty-lines": "error", // 여러줄 공백 금지
    "no-console": ["error", {"allow": ["warn", "error", "info"]}], // console 금지
    "eqeqeq": "error"
  }
}
